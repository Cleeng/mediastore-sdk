{
  "name": "@cleeng/mediastore-sdk",
  "version": "2.0.0",
  "private": false,
  "main": "dist/package.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Cleeng/mediastore-sdk"
  },
  "peerDependencies": {
    "react": ">= 18.2.0",
    "styled-components": ">= 4"
  },
  "dependencies": {
    "@babel/core": "^7.17.5",
    "@reduxjs/toolkit": "^1.7.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "camelcase": "^5.3.1",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "glob": "^7.2.0",
    "i18next": "^17.3.1",
    "i18next-browser-languagedetector": "^4.3.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-watch-typeahead": "0.3.1",
    "jwt-decode": "^2.2.0",
    "lodash.merge": "^4.6.2",
    "mkdirp": "^0.5.5",
    "prettier": "^1.19.1",
    "prop-types": "^15.8.1",
    "react-app-polyfill": "^1.0.6",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^10.13.2",
    "react-loading-skeleton": "^2.2.0",
    "react-redux": "^7.2.6",
    "react-select": "^4.3.1",
    "redux": "^4.1.2",
    "resolve": "1.22.0",
    "svgo": "^2.8.0"
  },
  "scripts": {
    "test": "node scripts/test.js",
    "scan-translations": "node -e 'require(`./scripts/translationsBundle.js`).run(false)'",
    "clean": "rimraf dist",
    "compile": "npm run clean && cross-env NODE_ENV=production babel src --ignore spec.js,test.js,stories.js --out-dir dist --copy-files ",
    "publish-dev": "npm run compile && yalc publish --push"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test -- --watchAll=false"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --no-ignore --fix",
      "git add --force"
    ],
    "*.{json,md,graphql}": [
      "prettier --write",
      "git add --force"
    ],
    "*.{css,less,styl,scss,sass,sss}": [
      "stylelint",
      "git add --force"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.17.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.1.0",
    "jest-styled-components": "^6.3.4",
    "lint-staged": "^9.5.0",
    "react": "^18.2.0",
    "regenerator-runtime": "^0.13.9",
    "styled-components": "^4.4.1",
    "stylelint": "^14.13.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  }
}
