{
  "name": "@cleeng/mediastore-sdk",
  "version": "0.0.0-development",
  "private": false,
  "type": "module",
  "main": "./dist/cleeng-mediastore-sdk.umd.cjs",
  "module": "./dist/cleeng-mediastore-sdk.js",
  "exports": {
    ".": {
      "import": "./dist/cleeng-mediastore-sdk.js",
      "require": "./dist/cleeng-mediastore-sdk.umd.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Cleeng/mediastore-sdk"
  },
  "peerDependencies": {
    "react": ">= 18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": ">= 5"
  },
  "dependencies": {
    "@adyen/adyen-web": "~5.38.0",
    "@reduxjs/toolkit": "^1.7.2",
    "camelcase": "^5.3.1",
    "classnames": "^2.3.2",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "glob": "^7.2.0",
    "i18next": "^22.4.13",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.0",
    "identity-obj-proxy": "3.0.0",
    "jwt-decode": "^3.1.2",
    "lodash.merge": "^4.6.2",
    "mixpanel-browser": "^2.48.1",
    "prop-types": "^15.8.1",
    "react-app-polyfill": "^1.0.6",
    "react-i18next": "^12.2.0",
    "react-loading-skeleton": "^3.1.0",
    "react-redux": "^7.2.6",
    "react-select": "^5.5.4",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.2",
    "resolve": "1.22.0"
  },
  "scripts": {
    "test": "vitest",
    "test:ci": "vitest run",
    "clean": "rimraf dist",
    "publish-dev": "pnpm build && yalc publish --push",
    "ts-coverage-ts": "typescript-coverage-report --project \"./tsconfig.json\" --ignore-files \"src/**/*.js*\"",
    "ts-coverage-js": "typescript-coverage-report --project \"./tsconfig.json\" --ignore-files \"src/**/*.ts*\"",
    "ts-check": "tsc --skipLibCheck --noEmit --pretty",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint-fix": "pnpm lint --fix",
    "dev": "pnpm run --prefix ../mediastore-sdk-demo-2024 dev & onchange './src/**' -e './src/**/*.(spec|test|type|types).[jt]s?(x)' -- pnpm publish-dev",
    "build": "pnpm clean && vite build",
    "dev:vite": "vite dev",
    "preview": "vite preview"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": ".husky/verify_branch_name && lint-staged && pnpm run test:ci",
      "commit-msg": ".husky/verify_commit_message && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --no-ignore --fix",
      "prettier --write",
      "git add --force"
    ],
    "*.{ts,tsx}": [
      "bash -c 'pnpm ts-check'",
      "eslint --fix",
      "prettier --write",
      "git add --force"
    ],
    "*.{json,md,graphql}": [
      "prettier --write",
      "git add --force"
    ],
    "*.{css,less,styl,scss,sass,sss}": [
      "stylelint",
      "git add --force"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.12",
    "@types/jwt-decode": "^3.1.0",
    "@types/mixpanel-browser": "^2.47.5",
    "@types/node": "^20.12.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.25",
    "@types/redux-mock-store": "^1.0.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^9.5.0",
    "onchange": "^7.1.0",
    "prettier": "^2.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redux-mock-store": "^1.5.4",
    "regenerator-runtime": "^0.13.9",
    "styled-components": "^5.3.6",
    "stylelint": "^14.13.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.9.5",
    "typescript-coverage-report": "^0.7.0",
    "vite": "^5.2.12",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^1.6.0"
  }
}
