{
  "name": "@cleeng/mediastore-sdk",
  "version": "0.0.0-development",
  "private": false,
  "main": "dist/package.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Cleeng/mediastore-sdk"
  },
  "peerDependencies": {
    "react": ">= 18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": ">= 5"
  },
  "dependencies": {
    "@adyen/adyen-web": "~5.38.0",
    "@reduxjs/toolkit": "^1.7.2",
    "camelcase": "^5.3.1",
    "classnames": "^2.3.2",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "glob": "^7.2.0",
    "i18next": "^22.4.13",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest-fetch-mock": "^3.0.3",
    "jwt-decode": "^3.1.2",
    "lodash.merge": "^4.6.2",
    "mixpanel-browser": "^2.48.1",
    "prop-types": "^15.8.1",
    "react-app-polyfill": "^1.0.6",
    "react-google-recaptcha": "^3.1.0",
    "react-i18next": "^12.2.0",
    "react-loading-skeleton": "^3.1.0",
    "react-redux": "^7.2.6",
    "react-select": "^5.5.4",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.2",
    "resolve": "1.22.0",
    "svgo": "^2.8.0"
  },
  "scripts": {
    "test": "jest test --watchAll=false",
    "clean": "rimraf dist",
    "compile": "npm run clean && cross-env NODE_ENV=production babel src --ignore spec.js,test.js,stories.js --out-dir dist --copy-files --extensions '.ts,.tsx,.js'",
    "publish-dev": "npm run compile && yalc publish --push",
    "ts-coverage-ts": "typescript-coverage-report --project \"./tsconfig.json\" --ignore-files \"src/**/*.js*\"",
    "ts-coverage-js": "typescript-coverage-report --project \"./tsconfig.json\" --ignore-files \"src/**/*.ts*\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": ".husky/verify_branch_name && lint-staged && npm run test",
      "commit-msg": ".husky/verify_commit_message && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --no-ignore --fix",
      "git add --force"
    ],
    "*.{json,md,graphql}": [
      "prettier --write",
      "git add --force"
    ],
    "*.{css,less,styl,scss,sass,sss}": [
      "stylelint",
      "git add --force"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "@types/jwt-decode": "^3.1.0",
    "@types/mixpanel-browser": "^2.47.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/react-redux": "^7.1.25",
    "@types/redux-mock-store": "^1.0.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "^3.1.0",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redux-mock-store": "^1.5.4",
    "regenerator-runtime": "^0.13.9",
    "styled-components": "^5.3.6",
    "stylelint": "^14.13.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.9.5",
    "typescript-coverage-report": "^0.7.0"
  }
}
