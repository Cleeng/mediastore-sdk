{
  "name": "@cleeng/mediastore-sdk",
  "version": "0.0.0-development",
  "private": false,
  "main": "dist/package.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Cleeng/mediastore-sdk"
  },
  "peerDependencies": {
    "react": ">= 18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": ">= 5"
  },
  "dependencies": {
    "@adyen/adyen-web": "~5.57.0",
    "@reduxjs/toolkit": "^2.2.1",
    "classnames": "^2.5.1",
    "i18next": "^23.8.2",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.3",
    "identity-obj-proxy": "3.0.0",
    "jest-fetch-mock": "^3.0.3",
    "jwt-decode": "^4.0.0",
    "lodash.merge": "^4.6.2",
    "mixpanel-browser": "^2.49.0",
    "prop-types": "^15.8.1",
    "react-i18next": "^14.0.5",
    "react-loading-skeleton": "^3.4.0",
    "react-redux": "^9.1.0",
    "react-select": "^5.8.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "resolve": "1.22.8",
    "svgo": "^3.2.0"
  },
  "scripts": {
    "test": "jest test --watchAll=false",
    "clean": "rimraf dist",
    "compile": "npm run clean && cross-env NODE_ENV=production babel src --ignore spec.js,test.js,stories.js --out-dir dist --copy-files --extensions '.ts,.tsx,.js'",
    "publish-dev": "npm run compile && yalc publish --push",
    "ts-coverage-ts": "typescript-coverage-report --project \"./tsconfig.json\" --ignore-files \"src/**/*.js*\"",
    "ts-coverage-js": "typescript-coverage-report --project \"./tsconfig.json\" --ignore-files \"src/**/*.ts*\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --no-ignore --fix",
      "git add --force"
    ],
    "*.{json,md,graphql}": [
      "prettier --write",
      "git add --force"
    ],
    "*.{css,less,styl,scss,sass,sss}": [
      "stylelint",
      "git add --force"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/plugin-transform-runtime": "^7.23.9",
    "@babel/preset-env": "7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/jwt-decode": "^3.1.0",
    "@types/mixpanel-browser": "^2.49.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-redux": "^7.1.33",
    "@types/redux-mock-store": "^1.0.6",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redux-mock-store": "^1.5.4",
    "regenerator-runtime": "^0.14.1",
    "styled-components": "^6.1.8",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^5.3.3",
    "typescript-coverage-report": "^1.0.0"
  }
}
