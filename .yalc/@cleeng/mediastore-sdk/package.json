  "scripts": {
    "test": "jest --watch",
    "scan-translations": "node -e 'require(`./scripts/translationsBundle.js`).run(false)'",
    "clean": "rimraf dist",
    "compile": "npm run clean && cross-env NODE_ENV=production babel src --ignore spec.js,test.js,stories.js --out-dir dist --copy-files ",
